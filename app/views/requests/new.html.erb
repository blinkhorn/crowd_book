<h2>New Request</h2>
<%= form_for @request do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :soundcloud %>
  <%= f.text_field :soundcloud %>

  <%= f.label :facebook %>
  <%= f.text_field :facebook %>

  <%= f.submit "Create" %>
<% end %>
