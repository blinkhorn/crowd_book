<h2>New User</h2>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password_digest %>
  <%= f.password_field :password_digest %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.submit "Create" %>
<% end %>
